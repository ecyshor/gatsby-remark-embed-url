{
    "name": "gatsby-remark-embed-url",
    "description": "Gatsby plugin to embed script output",
    "version": "1.1.2",
    "author": "Nicu Reut <contact@nicu.dev>",
    "repository": {
        "type": "git",
        "url": "https://github.com/ecyshor/gatsby-remark-embed-url.git"
    },
    "dependencies": {
        "@babel/runtime": "^7.23.9",
        "lz-string": "^1.5.0",
        "node-fetch": "^3.3.2",
        "normalize-path": "^3.0.0",
        "npm-package-arg": "^11.0.1",
        "recursive-readdir": "^2.2.3",
        "unist-util-map": "^2.0.1",
        "urijs": "^1.19.11"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "babel-preset-gatsby-package": "^3.13.1",
        "cross-env": "^7.0.2"
    },
    "main": "index.js",
    "peerDependencies": {
        "gatsby": "^5.0.0"
    },
    "scripts": {
        "build": "babel src --out-dir . --ignore \"**/__tests__\"",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-remark",
        "gatsby-embed-script",
        "gatsby-embed-gist",
        "gatsby-embed-url"
    ],
    "license": "MIT"
}
